{
  "name": "Njuskalo Kvadrati",
  "description": "Prikaz cijene po kvadratu na Njuskalo oglasniku za nekretnine.",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "images/16x16.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "background": {
    "service_worker": "./background.js"
  },
  "action": {
    "default_popup": "./popup.html",
    "default_icons": {
      "16": "images/16x16.png",
      "48": "images/48x48.png",
      "128": "images/128x128.png"
    }
  },
  "permissions": ["scripting"],
  "host_permissions": ["https://www.njuskalo.hr/*"]
}
